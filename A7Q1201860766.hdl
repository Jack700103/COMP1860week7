CHIP DecoderSegment201860766 {
    IN A, B, C, D;
    OUT C_out;

    PARTS:
        Not(in=B, out=NB);
        Not(in=C, out=NC);
        Not(in=A, out=NA);

        And(a=NC, b=D, out=term2);
        And(a=A, b=NC, out=term3);
        And(a=NA, b=C, out=term4);

